*{
  margin:0;
  padding:0;
}
wrapper{
	width:100px;
	height:100px;
	margin:auto;
}
header{
	background: linear-gradient(rgba(0,0,0,0.6),rgba(0,0,0,0.6)), url("print.gif");
	height:100vh;
	-webkit background-size:cover;
	background-size:cover;
	background-position:center center;
    position:relative;
}

/*nav bar */
.nav-area{
	float:right;
	list-style:none;
	margin-top:20px;
	
}
.nav-area li{
	display: inline-block;
	
}
.nav-area li a{
	color:#fff;
	text-decoration:none;
	padding:5px 20px;
	font-family:poppins;
	font-size:18px;
	border-radius:10px;
	
}
.nav-area li a:hover{
	background: #fff;
	color: #333;
	
}
.logo img{
	width:100px;
	float: left;
	height: auto;
	background: rgba(0,0,0,0.5);
	border-radius: 50%;
		
}
/* Welcome Text */
.welcome-text{
	position:absolute;
	width:600px;
	height:300px;
	margin: 20% 30%;
	text-align:center
	
}
.welcome-text h2{
	text-align: center;
	color:#fff;
	text-transform: uppercase;
	font-size:50px;
	
}
.welcome-text a{
	border: 1px solid #fff;
	padding: 10px 25px;
	text-decoration:none;
	text-transform: uppercase;
	font-size:16px;
	margin-top:20px;
	display:inline-block;
	color:#fff;
	margin:20%;
	
}
.welcome-text a:hover{
	background: #fff;
	color:#333;
	
}
/*---search bar--*/
.search_bar{
	text-align:center;
	position:absolute;
	margin: 34% 50%;
	transform:translate(-50%,-50%);
	width:300px;
	background:rgba(0,0,0,0.6);
	max-width:550px;
	width:100%;
	padding:15px;
	border-radius:5px;
	display: flex;
	justify-content:space-between;	
	
}

.search_bar input{
	width:85%;
	padding:15px 20px;
	border:none;
	font-weight:bold;
}

.search-btn{
	position:absolute;
	top: 19%;
	left: 85%;
	font-size:18px;
	height:50px;
	background-color:#ffec00;
	border-radius:5px;
	width:80px;	
}
/*-----about-------*/


/* browse buttons styles */
.Browse-header {
	position:absolute;
	top: 89%;
	margin-left: 150px;
}

.Browse-header-text {
	font-family: helvetica;
	font-size: 18px;
	color: white;
	margin-left:25px;
	
}

.browse-btns {
	margin-top: 10px;
	
}

.btn {
	display: inline-flex;
	height: 45px;
	margin-right: 7px;
	padding: 0;
	background: rgba(0, 0, 0, 0.8);
	color: black;
	border: none;
	outline: none;
	overflow: hidden;
	font-size: 16px;
	cursor: pointer;
	transition: all 0.5s ease 0s;
	border-radius:50px;
	text-decoration:none;
	
}

.btn:hover {
	background: #f85959;
	text-decoration:none;
}

.btn-text {
	display: inline-flex;
	align-items: center;
	padding: 0 14px;
	height: 100%;
}

.btn-icon {
	display: inline-flex;
	align-items: center;
	padding: 0 0 0 14px;
	height: 100%;
}

.browse-btn-anchors {
	color: white;
	font-family: "Poppins", Arial, sans-serif;
	font-weight: 200;
}

.container {
	margin-bottom: 190px;
}

/* Gallery card styles  */
.container3 {
	background: #f8faff;
	margin-top: 1000px;
	width: 90%;
	margin: 50px auto;
	font-family:"Poppins", Arial, sans-serif;
}

h1 {
	padding-top: 30px;
	margin-left: 75px;
	margin-bottom: 0;
	font-family: helvetica;
	font-size: 40px;
	color: black;
	font-weight: 500;
}
a{
	text-decoration:none;
	color:#f85959;
}
a:hover{
	color:black;
}

#type-gallerySub {
	margin-top: 0;
	margin-bottom: 20px;
	margin-left: 75px;
	color: black;
}

.row1 {
	display: flex;
	flex-direction: row;
	justify-content: space-around;
	flex-flow: wrap;
	padding-top:10px;
}

.gallery_card {
	width: 25%;
	background: #fff;
	margin-bottom: 50px;
	transition: 0.3s;
}

.gallery_card:hover {
	transform: scale(1.1);
	box-shadow: 0 0 40px -10px rgba(0, 0, 0, 0.35);
	background: #f85959;
	color: white;
	font-weight: 700;
}

.card_images {
	width: 100%;
	height: 200px;
}

.description_headers {
	margin-bottom: 10px;
}

.card-img {

}

.card-body {
	padding: 30px 20px;
	text-align: center;
	font-size: 18px;
	font-weight: 200;
}

@media screen and (max-width: 1000px) {
.container3 {
		width: 100%;
	}

.gallery_card {
		width: 80%;
	}
}


