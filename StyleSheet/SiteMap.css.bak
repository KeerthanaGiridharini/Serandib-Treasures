body {
	background-image: url(background.jpg);
	background-size: cover;
	background-repeat: no-repeat;
	background-attachment: fixed;
}

            
#svgJS g:hover polygon,
#svgJS g:hover text,
#svgJS g:hover rect,
#svgJS g:focus polygon,
#svgJS g:focus rect {
	fill:hsla(24, 20%, 50%, .3);
	stroke:black;
	stroke-width:2;
	cursor: pointer;
}

footer {
	position: absolute;
	margin-top: 50px;
	margin-bottom: 0;
	background: #36718F;
	height: auto;
	width: 100%;
	font-family: "Poppins", Arial, sans-serif;
	padding-top: 40px;
	color: #fff;
}

.footer-content {
    display: flex;
	text-align: center;
	align-items: center;
	justify-content: center;
	flex-direction: column;
}

.footer-content h3 {
    font-size: 30px;
	font-family: monospace;
}

.footer-content p {
    display: flex;
	margin: 10px auto;
	font-size: 14px;
	font-family:monospace;
}

.footer_bottom {
    margin-top: 40px;
	width: 100%;
	padding: 20px 0;
}

.footer_bottom p {
    text-align: center;
	font-size: 14px;
	word-spacing: 2px;
	color:white;
	padding-bottom:20px;

}
.footer_bottom p a:hover{
	color:black;
	text-decoration:none;
}


.footer_bottom span {
    opacity: .5;
}
            
.logo2 img {
    width:100px;
	float: left;
	height: auto;
	background: rgba(0,0,0,0.5);
	border-radius:50px;
}
            
.logo3 fa{
    display: inline-flex;
	width:50px;
	height:50px;
    display: inline-block;
    padding-left: 3em;
	padding: 5px;
}
.logo3 a{
	color:white;
	text-decoration:none;
}
.logo3 a:hover{
	color:black;
}

.logo3 i{
    padding: 5px;
}


/* back to top button styles */
#to_top_btn {
    display: none;
    position: fixed;
    bottom: 20px;
    right: 20px;
    z-index: 99;
    font-size: 15px;
    border: none;
    outline: none;
    background-color: #3DA4FF;
    color: white;
    cursor: pointer;
    padding: 10px;
}

#to_top_btn:hover {
    background-color: #f85959;
}

header{
  position: absolute;
  top: 0; left:0;right:0;
  display:flex;
  justify-content: space-between;
  padding: 4px 10px;
  background: #36718F;
  z-index:1;
  font-family:Poppins;

}
.logo{
 width:100px;
 	float: left;
 	height: auto;
 	background: rgba(0,0,0,0.5);
 	border-radius:50%;
}
.navbar {
  overflow: hidden;
  padding-top: 15px;
 
}

.navbar a {
  float: left;
  font-size: 18px;
  color: white;
  text-align: center;
  padding: 14px 16px;
  text-decoration: none;
   font-family:poppins;
}

.dropdown {
  float: left;
  overflow: hidden;
}

.dropdown .dropbtn {
  font-size: 18px;
  border: none;
  outline: none;
  color: white;
  padding: 14px 16px;
  background-color: inherit;
  font-family: Poppins;
  margin: 0;
  
}

.navbar a:hover, .dropdown:hover .dropbtn {
  background-color: white;
  color:black;
  border-radius:8px;
}

.dropdown-content {
  display: none;
  position: absolute;
  background-color: #f9f9f9;
  min-width: 160px;
  box-shadow: 0px 8px 16px 0px rgba(0,0,0,0.2);
  z-index: 1;
}

.dropdown-content a {
  float: none;
  color: black;
  padding: 12px 16px;
  text-decoration: none;
  display: block;
  text-align: left;
}

.dropdown-content a:hover {
  background-color: #ddd;
}

.dropdown:hover .dropdown-content {
  display: block;
}

